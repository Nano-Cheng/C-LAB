      </Atoms>
    </UtilsFunction>
    <UtilsFunction DisplayName="启动色谱Link1" DisplayClass="Link设定" Range="Manual" Filter="ShortPanel" Enable="!('$#this#.DataSystem$'=='None')">
      <Parameters />
      <Atoms>
        <Atom>System.ExecOSCmd(CmdName="$System._RunDir$\Resource\Links\Link $System.DataSystem$\Link $System.DataSystem$.exe",Args="-p7600")</Atom>
      </Atoms>
    </UtilsFunction>
    <UtilsFunction DisplayName="启动色谱Link2" DisplayClass="Link设定" Range="Manual" Filter="ShortPanel" Enable="!('$#this#.DataSystem2$'=='None')">
      <Parameters />
      <Atoms>
        <Atom>If('$System.DataSystem$'!='$System.DataSystem2$')</Atom>
        <!-- -d表示Link2 -->
        <Atom>System.ExecOSCmd(CmdName="$System._RunDir$\Resource\Links\Link $System.DataSystem2$\Link $System.DataSystem2$.exe",Args="-d -p6600 -i1")</Atom>
        <Atom>Else()</Atom>
        <Atom>System.Wait(Time=5,Info=)</Atom>
        <Atom>System.ExecOSCmd(CmdName="$System._RunDir$\Resource\Links\Link $System.DataSystem2$\Link $System.DataSystem2$.exe",Args="-d -p6600 -i2")</Atom>
        <Atom>EndIf()</Atom>
      </Atoms>
    </UtilsFunction>
    <UtilsFunction DisplayName="停用自动发送" DisplayClass="Link设定" IsLongTime="False" Range="Manual" Filter="ShortPanel" Enable="'$#this#.DataSystem$'!='None' || '$#this#.DataSystem2$'!='None'">
      <Parameters />
      <Atoms>
        <Atom>System.SetSaveInsField(InsName=#this#,VarName=$DataSystem$,VarValue=None)</Atom>
        <Atom>System.SetSaveInsField(InsName=#this#,VarName=$DataSystem2$,VarValue=None)</Atom>
        <Atom>System.InfoDialog(Message="$System.SystemSettingOK$",ShowCancel=False)</Atom>
      </Atoms>
    </UtilsFunction>
    <UtilsFunction DisplayName="色谱1设定" DisplayClass="Link设定" IsLongTime="False" Range="Manual" Enable="True">
      <Parameters>
        <Parameter Token="$DSType$" Type="List" ValueList="$#this#.DSList$" DisplayName="色谱工作站" Default="$#this#.DataSystem$" DisableCondition="False" RefreshKey="" UniqueCondition="False" VisableCondition="True" SameCondition="False" />
      </Parameters>
      <Atoms>
        <Atom>System.SetSaveInsField(InsName=#this#,VarName=$DataSystem$,VarValue=$DSType$)</Atom>
      </Atoms>
    </UtilsFunction>
