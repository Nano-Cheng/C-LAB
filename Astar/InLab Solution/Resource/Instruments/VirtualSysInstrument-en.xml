      </Parameters>
      <Atoms>
        <Atom>System.SetSaveInsField(InsName=#this#,VarName=$DisableBlackBox$,VarValue=$DisableBB$)</Atom>
        <Atom>System.SetSaveInsField(InsName=#this#,VarName=$DefaultCamerName$,VarValue=$CamerName$)</Atom>
        <Atom>#this#.InfoDialog(Message="重启软件后生效")</Atom>
      </Atoms>
    </UtilsFunction>
