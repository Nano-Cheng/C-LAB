    <UtilsFunction DisplayName="黑匣子设置" DisplayClass="常用功能" Range="Manual" Enable="True">
      <Parameters>
        <Parameter Token="$DisableBB$" Type="Bool" DisplayName="禁用黑匣子" Default="False" DisableCondition="False" RefreshKey="" UniqueCondition="False" VisableCondition="True" SameCondition="False" />
